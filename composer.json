{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "askedio/laravel-soft-cascade": "^10.0.0",
        "barryvdh/laravel-ide-helper": "^v2.13.0",
        "doctrine/dbal": "^3.0",
        "dompdf/dompdf": "^v2.0.3",
        "graham-campbell/markdown": "^v15.0.0",
        "guzzlehttp/guzzle": "^7.2",
        "ifsnop/mysqldump-php": "^v2.10",
        "knplabs/knp-snappy": "^1.2",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.6",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.8",
        "laravolt/avatar": "^5.0.0",
        "livewire/livewire": "^3.0",
        "mollie/mollie-api-php": "^2.50",
        "rappasoft/laravel-livewire-tables": "^v3.1.1",
        "simshaun/recurr": "^v5.0.0",
        "sonata-project/exporter": "^3.1.1",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-permission": "^5.9.1",
        "square/square": "^34.0",
        "srmklive/paypal": "^3.0.19",
        "stripe/stripe-php": "^v13.10.0",
        "tightenco/parental": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "fakerphp/faker": "^1.13",
        "laravel/pint": "^1.10",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/factories"
        ],
        "psr-4": {
            "BT\\": "app/",
            "Addons\\": "custom/addons/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/thanks": false
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
