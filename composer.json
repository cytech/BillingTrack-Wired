{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "askedio/laravel-soft-cascade": "^9.0.0",
        "barryvdh/laravel-ide-helper": "^2.9",
        "doctrine/dbal": "^3.0",
        "dompdf/dompdf": "^v2.0.0",
        "graham-campbell/markdown": "^v14.0.0",
        "guzzlehttp/guzzle": "^7.2",
        "ifsnop/mysqldump-php": "2.*",
        "knplabs/knp-snappy": "^1.2",
        "laravel/framework": "^9.2",
        "laravel/helpers": "^1.4",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.2",
        "laravolt/avatar": "^4.1",
        "livewire/livewire": "^2.7",
        "mollie/mollie-api-php": "^2.29",
        "rappasoft/laravel-livewire-tables": "^v2.7.0",
        "simshaun/recurr": "^v5.0.0",
        "sonata-project/exporter": "^2.5",
        "spatie/laravel-permission": "^5.5",
        "srmklive/paypal": "~3.0",
        "stripe/stripe-php": "^7.69"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "nunomaduro/collision": "^6.1",
        "barryvdh/laravel-debugbar": "^3.5",
        "fakerphp/faker": "^1.13",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "classmap": [
            "database/factories"
        ],
        "psr-4": {
            "BT\\": "app/",
            "Addons\\": "custom/addons/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/thanks": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
